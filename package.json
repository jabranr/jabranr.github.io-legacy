{
  "name": "jabranr.github.io",
  "version": "2.0.0",
  "description": "Jabran Rafique's personal website and blog",
  "main": "src/index.js",
  "scripts": {
    "dev": "gatsby develop --verbose",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "test": "jest",
    "build": "gatsby build",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jabranr/jabranr.github.io.git"
  },
  "keywords": [
    "blog",
    "website",
    "Jabran",
    "Rafique"
  ],
  "author": "Jabran Rafique <hello@jabran.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jabranr/jabranr.github.io/issues"
  },
  "private": true,
  "homepage": "https://www.jabran.me",
  "devDependencies": {
    "classnames": "^2.2.6",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "gatsby": "^2.18.7",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-feed": "^2.3.23",
    "gatsby-plugin-google-tagmanager": "^2.1.18",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sass": "^2.1.24",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-typography": "^2.3.18",
    "gatsby-remark-copy-linked-files": "^2.1.31",
    "gatsby-remark-images": "^3.1.35",
    "gatsby-remark-prismjs": "^3.3.27",
    "gatsby-remark-responsive-iframe": "^2.2.28",
    "gatsby-remark-smartypants": "^2.1.17",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-remark": "^2.6.42",
    "gatsby-transformer-sharp": "^2.1.3",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.13",
    "typeface-merriweather": "0.0.72",
    "typeface-roboto": "0.0.75",
    "typography": "^0.16.17"
  }
}
